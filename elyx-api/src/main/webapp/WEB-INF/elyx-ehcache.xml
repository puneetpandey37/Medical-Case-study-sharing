	      maxBytesLocalHeap="100M"
	      eternal="false"
	      timeToIdleSeconds="300"
	      overflowToDisk="true"
	      maxElementsOnDisk="1000"	      
	      memoryStoreEvictionPolicy="LRU"/>	         
	      
	      <cache name="posts"
	      maxBytesLocalHeap="100M"
	      eternal="false"
	      timeToIdleSeconds="300"
